<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.zaozi.jwms.dao.StudentDao">
    <select id="selectByNumAndPwd" resultType="com.qf.zaozi.jwms.pojo.Student">
        select * from tb_manager where stu_num = #{snum} and stu_login_pwd = #{pwd};
    </select>
</mapper>